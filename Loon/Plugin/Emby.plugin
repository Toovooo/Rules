[Script]
http-response ^https:\/\/mb3admin\.com\/admin\/service(\/registration\/validateDevice|\/appstore\/register|\/registration\/validate|\/registration\/getStatus|\/supporter\/retrievekey) script-path=https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Embycrack.js, requires-body=true, timeout=10, tag=Emby
http-response \/Users\/\w{32}\/Items\/\d+\? script-path=(\r\n)Accept-Encoding:.+ request-header $Accept-Encoding: gzip, deflate, br, requires-body=true, timeout=10, tag=Emby
http-response \/Users\/\w{32}\/Items\/\d+\? script-path=https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby-plugin.js, requires-body=true, timeout=10, tag=Emby
http-response \/plugin\/scheme url script-echo-response https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby-plugin.js, requires-body=true, timeout=10, tag=Emby
http-response (\/Videos\/\d+\/stream\/.+\?.+&filename)|(\/Videos\/\d+\/\w{32}\/Subtitles\/\d+\/.+\?.+&filename) script-path=https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby-plugin.js, requires-body=true, timeout=10, tag=Emby
http-response (\/Videos\/\d+\/stream\/.+\?.+&filename)|(\/Videos\/\d+\/\w{32}\/Subtitles\/\d+\/.+\?.+&filename) script-path=https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Download-file-rename.js, requires-body=true, timeout=10, tag=Emby

[Mitm]
hostname = mb3admin.com, *.emby.wtf, *.plusmedia.site, *.jmsooo.com, embymv.tk, *.misakaf.org, nakonako.net, nakoemby.com, cu.poloemby.xyz
