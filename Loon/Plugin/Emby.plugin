#!name = Emby
#!desc = 解锁播放,支持外连播放器
#!icon = https://raw.githubusercontent.com/Toovooo/Rules/master/Icon/Black/Emby.png

[Rule]
DOMAIN-SUFFIX, ju.dog, PROXY
DOMAIN-SUFFIX, cf2.jmsooo.com, PROXY
DOMAIN-SUFFIX, embymv.tk, PROXY

[Script]
http-response ^https:\/\/mb3admin\.com\/admin\/service(\/registration\/validateDevice|\/appstore\/register|\/registration\/validate|\/registration\/getStatus|\/supporter\/retrievekey) script-path = https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby/EmbyCrack.js, requires-body = true, tag = 播放解锁
http-response \/Users\/\w{32}\/Items\/\d+\? script-path = https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby/EmbyPlugin.js, requires-body = true, tag = 添加外部播放器连接
http-response \/plugin\/scheme script-path = https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby/EmbyPlugin.js, requires-body = true, tag = 打开外部播放器
http-response (\/Videos\/\d+\/stream\/.+\?.+&filename)|(\/Videos\/\d+\/\w{32}\/Subtitles\/\d+\/.+\?.+&filename) script-path = https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby/EmbyPlugin.js, requires-body = true, tag = 资源路径伪静态
http-response (\/Videos\/\d+\/stream\?.+&filename)|(\/Videos\/\d+\/\w{32}\/Subtitles\/.+&filename) script-path = https://raw.githubusercontent.com/Toovooo/Rules/master/Script/Emby/EmbyDownload.js, requires-body = true, tag = 下载文件重命名

[Mitm]
hostname = mb3admin.com, *.jmsooo.com, embymv.tk, ju.dog
